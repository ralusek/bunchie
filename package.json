{
  "name": "bunchie",
  "version": "3.0.0",
  "description": "A keyed debouncer.",
  "keywords": [
    "bunch",
    "batch",
    "accrue",
    "accumulate",
    "pool",
    "agglomerate",
    "stackup",
    "collect",
    "debouce",
    "throttle"
  ],
  "author": "Tomas Savigliano",
  "license": "MIT",
  "main": "./lib/index.js",
  "files": [
    "/lib"
  ],
  "scripts": {
    "build": "npx ttsc",
    "start": "npm run build",
    "test": "npm run build && mocha -r ts-node/register tests/**/*.test.ts",
    "prepublish": "npm run build",
    "prettify": "npx prettier --write '**/*.ts'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run build"
    }
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.13",
    "@types/validator": "^13.7.1",
    "chai": "^4.3.6",
    "husky": "^7.0.4",
    "mocha": "^9.2.0",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.5",
    "typescript-transform-paths": "^3.3.1"
  }
}
